{
  "functions": {
    "main.py": {
      "runtime": "python:3.9"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/extract-audio",
      "dest": "/api/extract-audio"
    },
    {
      "src": "/search/:query",
      "dest": "/api/search/:query"
    },
    { "src": "/(.*)", "dest": "/main.py" }
  ]
}
